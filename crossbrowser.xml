<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite"  thread-count="5" parallel="true" >
  <test name="ChromeTest">
	<parameter name="browser" value="chrome"/>
    <classes>
      <class name="test.ClientPageTestCase"/>
      <class name="test.UserPageTestCase"/>
      <class name="test.BugPageTestCase"/>
      <class name="test.FileManagerTestCase"/>
      <class name="test.BaseSetup"/>
    </classes>
  </test> <!-- Test -->
  <test name="FirefoxTest">
	<parameter name="browser" value="firefox"/>
    <classes>
      <class name="test.ClientPageTestCase"/>
      <class name="test.UserPageTestCase"/>
      <class name="test.BugPageTestCase"/>
      <class name="test.FileManagerTestCase"/>
      <class name="test.BaseSetup"/>
    </classes>
  </test>
</suite> <!-- Suite -->
